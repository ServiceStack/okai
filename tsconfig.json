{
    "compilerOptions": {
      "outDir": "./dist/",
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
  }